{
  "compilerOptions": {
    "rootDir": ".",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "es2022",
    "module": "esnext",
    "lib": ["es2020", "dom"],
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "baseUrl": ".",
    "paths": {
      "@bv-admin-app/shared/config": ["src/app/shared/config/index.ts"],
      "@bv-admin-app/shared/model": ["src/app/shared/model/index.ts"],
      "@bv-admin-app/shared/http": ["src/app/shared/http/index.ts"],
      "@bv-admin-app/shared/messages/api": ["src/app/shared/messages/api/index.ts"],
      "@bv-admin-app/shared/messages/ui": ["src/app/shared/messages/ui/index.ts"],
      "@bv-admin-app/shared/auth/api": ["src/app/shared/auth/api/index.ts"],
      "@bv-admin-app/shared/auth/model": ["src/app/shared/auth/model/index.ts"],
      "@bv-admin-app/shared/auth/data": ["src/app/shared/auth/data/index.ts"],
      "@bv-admin-app/shared/ui": ["src/app/shared/ui/index.ts"],
      "@bv-admin-app/shared/util": ["src/app/shared/util/index.ts"],
      "@bv-admin-app/shared/local-storage-data": ["src/app/shared/local-storage-data/index.ts"],
      "@bv-admin-app/benutzer/api": ["src/app/benutzer/api/index.ts"],
      "@bv-admin-app/benutzer/feature": ["src/app/benutzer/feature/index.ts"],
      "@bv-admin-app/benutzer/data": ["src/app/benutzer/data/index.ts"],
      "@bv-admin-app/benutzer/model": ["src/app/benutzer/model/index.ts"],
      "@bv-admin-app/infomails/model": ["src/app/infomails/model/index.ts"],
      "@bv-admin-app/infomails/data": ["src/app/infomails/data/index.ts"],
      "@bv-admin-app/infomails/api": ["src/app/infomails/api/index.ts"],
      "@bv-admin-app/infomails/feature": ["src/app/infomails/feature/index.ts"],
      "@bv-admin-app/versandauftraege/model": ["src/app/versandauftraege/model/index.ts"],
      "@bv-admin-app/versandauftraege/data": ["src/app/versandauftraege/data/index.ts"],
      "@bv-admin-app/versandauftraege/api": ["src/app/versandauftraege/api/index.ts"]
    },
    "useDefineForClassFields": false,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "noImplicitOverride": true,
    "noPropertyAccessFromIndexSignature": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true
  },
  "files": [],
  "include": [],
  "references": [
    {
      "path": "./tsconfig.editor.json"
    },
    {
      "path": "./tsconfig.app.json"
    },
    {
      "path": "./tsconfig.spec.json"
    }
  ],
  "compileOnSave": false,
  "exclude": ["node_modules", "tmp"],
  "angularCompilerOptions": {
    "enableI18nLegacyMessageIdFormat": false,
    "strictInjectionParameters": true,
    "strictInputAccessModifiers": true,
    "strictTemplates": true
  }
}
