{
  "name": "authprovider-ws",
  "version": "8.0.1",
  "license": "MIT",
  "scripts": {
    "ng": "nx",
    "postinstall": "node ./decorate-angular-cli.js && ngcc --properties es2015 browser module main --first-only --create-ivy-entry-points",
    "nx": "nx",
    "start": "ng serve",
    "build:auth-app:local": "npx nx build auth-app -c heikedeb --base-href /auth-app --deploy-url auth-app/ --skip-nx-cache",
    "build:profil-app:local": "npx nx build profil-app -c heikedeb --base-href /profil-app --deploy-url profil-app/ --skip-nx-cache",
    "build:auth-app:heikeqs": "npx nx build auth-app -c heikeqs --output-hashing=all --base-href /auth-app --deploy-url auth-app/ --skip-nx-cache",
    "build:profil-app:heikeqs": "npx nx build profil-app -c heikeqs --output-hashing=all --base-href /profil-app --deploy-url profil-app/ --skip-nx-cache",
    "build:auth-app:prod": "npx nx build auth-app ---configuration production --output-hashing=all --base-href /auth-app --deploy-url auth-app/ --skip-nx-cache",
    "build:profil-app:prod": "npx nx build profil-app --configuration production --output-hashing=all --base-href /profil-app --deploy-url profil-app/ --skip-nx-cache",
    "test": "ng test",
    "lint": "nx workspace-lint && ng lint",
    "e2e": "ng e2e",
    "affected:apps": "nx affected:apps",
    "affected:libs": "nx affected:libs",
    "affected:build": "nx affected:build",
    "affected:e2e": "nx affected:e2e",
    "affected:test": "nx affected:test",
    "affected:lint": "nx affected:lint",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected": "nx affected",
    "format": "nx format:write",
    "format:write": "nx format:write",
    "format:check": "nx format:check",
    "update": "ng update @nrwl/workspace",
    "workspace-schematic": "nx workspace-schematic",
    "dep-graph": "nx dep-graph",
    "help": "nx help"
  },
  "private": true,
  "dependencies": {
    "@angular-devkit/architect": "^0.1202.2",
    "@angular/animations": "13.3.3",
    "@angular/common": "13.3.3",
    "@angular/compiler": "13.3.3",
    "@angular/core": "13.3.3",
    "@angular/forms": "13.3.3",
    "@angular/localize": "13.3.3",
    "@angular/platform-browser": "13.3.3",
    "@angular/platform-browser-dynamic": "13.3.3",
    "@angular/router": "13.3.3",
    "@ng-bootstrap/ng-bootstrap": "^11.0.0",
    "@nrwl/angular": "13.10.2",
    "bootstrap": "^4.5.0",
    "font-awesome": "^4.7.0",
    "ngx-cookie-service": "^12.0.3",
    "rxjs": "^7.4.0",
    "zone.js": "0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^13.3.9",
    "@angular-eslint/eslint-plugin": "13.0.1",
    "@angular-eslint/eslint-plugin-template": "13.0.1",
    "@angular-eslint/template-parser": "13.0.1",
    "@angular/cli": "^14.2.13",
    "@angular/compiler-cli": "13.3.3",
    "@angular/language-service": "13.3.3",
    "@nrwl/cli": "13.9.6",
    "@nrwl/cypress": "13.10.2",
    "@nrwl/eslint-plugin-nx": "13.10.2",
    "@nrwl/jest": "13.10.2",
    "@nrwl/linter": "13.10.2",
    "@nrwl/tao": "12.10.1",
    "@nrwl/workspace": "13.10.2",
    "@types/jest": "27.0.2",
    "@types/node": "14.14.33",
    "@typescript-eslint/eslint-plugin": "5.18.0",
    "@typescript-eslint/parser": "5.18.0",
    "cypress": "^8.3.0",
    "eslint": "8.12.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-cypress": "^2.10.3",
    "jest": "27.2.3",
    "jest-preset-angular": "11.1.1",
    "ng-packagr": "13.3.1",
    "nx": "13.10.2",
    "postcss": "^8.4.5",
    "postcss-import": "^14.0.2",
    "postcss-preset-env": "^6.7.0",
    "postcss-url": "^10.1.1",
    "prettier": "2.6.2",
    "ts-jest": "27.0.5",
    "typescript": "4.6.3"
  }
}
