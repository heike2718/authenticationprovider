{
  "name": "authprovider-ws",
  "version": "8.0.2",
  "license": "MIT",
  "scripts": {
    "ng": "nx",
    "start": "NX_DAEMON=false nx serve",
    "start:auth-app": "NX_DAEMON=false nx serve auth-app",
    "start:profil-app": "NX_DAEMON=false nx serve profil-app",
    "build:auth-app:heiketux": "npx nx build auth-app -c heiketux --base-href /auth-app --deploy-url auth-app/ --skip-nx-cache",
    "build:profil-app:heiketux": "npx nx build profil-app -c heiketux --base-href /profil-app --deploy-url profil-app/ --skip-nx-cache",
    "build:auth-app:heikeqs": "npx nx build auth-app -c heikeqs --output-hashing=all --base-href /auth-app --deploy-url auth-app/ --skip-nx-cache",
    "build:profil-app:heikeqs": "npx nx build profil-app -c heikeqs --output-hashing=all --base-href /profil-app --deploy-url profil-app/ --skip-nx-cache",
    "build:auth-app:prod": "npx nx build auth-app ---configuration production --output-hashing=all --base-href /auth-app --deploy-url auth-app/ --skip-nx-cache",
    "build:profil-app:prod": "npx nx build profil-app --configuration production --output-hashing=all --base-href /profil-app --deploy-url profil-app/ --skip-nx-cache",
    "test": "ng test",
    "lint": "nx workspace-lint && ng lint",
    "e2e": "ng e2e",
    "affected:apps": "nx affected:apps",
    "affected:libs": "nx affected:libs",
    "affected:build": "nx affected:build",
    "affected:e2e": "nx affected:e2e",
    "affected:test": "nx affected:test",
    "affected:lint": "nx affected:lint",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected": "nx affected",
    "format": "nx format:write",
    "format:write": "nx format:write",
    "format:check": "nx format:check",
    "update": "ng update @nrwl/workspace",
    "workspace-schematic": "nx workspace-schematic",
    "dep-graph": "nx dep-graph",
    "help": "nx help"
  },
  "private": true,
  "dependencies": {
    "@angular-devkit/architect": "0.1502.9",
    "@angular/animations": "15.2.9",
    "@angular/common": "15.2.9",
    "@angular/compiler": "15.2.9",
    "@angular/core": "15.2.9",
    "@angular/forms": "15.2.9",
    "@angular/localize": "15.2.9",
    "@angular/platform-browser": "15.2.9",
    "@angular/platform-browser-dynamic": "15.2.9",
    "@angular/router": "15.2.9",
    "@ng-bootstrap/ng-bootstrap": "14.2.0",
    "@nrwl/angular": "15.9.7",
    "bootstrap": "5.2.3",
    "font-awesome": "4.7.0",
    "rxjs": "7.8.1",
    "zone.js": "0.12.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "15.2.11",
    "@angular-eslint/eslint-plugin": "15.0.0",
    "@angular-eslint/eslint-plugin-template": "15.0.0",
    "@angular-eslint/template-parser": "15.0.0",
    "@angular/cli": "15.2.11",
    "@angular/compiler-cli": "15.2.9",
    "@angular/language-service": "15.2.9",
    "@nrwl/cli": "15.9.7",
    "@nrwl/cypress": "15.9.7",
    "@nrwl/eslint-plugin-nx": "15.9.7",
    "@nrwl/jest": "15.9.7",
    "@nrwl/linter": "15.9.7",
    "@nrwl/tao": "15.9.7",
    "@nrwl/workspace": "15.9.7",
    "@types/jest": "29.4.4",
    "@types/node": "14.14.33",
    "@typescript-eslint/eslint-plugin": "5.62.0",
    "@typescript-eslint/parser": "5.62.0",
    "cypress": "12.17.4",
    "eslint": "8.15.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-cypress": "2.15.2",
    "jest": "29.4.3",
    "jest-environment-jsdom": "28.1.3",
    "jest-preset-angular": "13.0.0",
    "ng-packagr": "15.2.2",
    "nx": "15.9.7",
    "postcss": "8.4.39",
    "postcss-import": "14.1.0",
    "postcss-preset-env": "7.5.0",
    "postcss-url": "10.1.3",
    "prettier": "2.6.2",
    "ts-jest": "29.0.5",
    "typescript": "4.8.4"
  }
}
