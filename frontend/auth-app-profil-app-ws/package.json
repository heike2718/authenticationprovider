  "scripts": {
    "auth": "npx nx serve authprovider --port 4300",
    "profil": "npx nx serve benutzerprofil --port 4400",
    "build:authprovider:local": "npx nx build authprovider -c local --output-hashing=all --base-href /authprovider/ --skip-nx-cache",
    "build:authprovider:docker": "npx nx build authprovider -c docker --output-hashing=all --base-href /authprovider/ --skip-nx-cache",
    "build:authprovider:qs": "npx nx build authprovider -c heikeqs --output-hashing=all --base-href /authprovider/ --skip-nx-cache",
    "build:authprovider:prod": "npx nx build authprovider:production --output-hashing=all --base-href /authprovider/ --skip-nx-cache",
    "build:benutzerprofil:local": "npx nx build benutzerprofil -c local --output-hashing=all --base-href /benutzerprofil/ --skip-nx-cache",
    "build:benutzerprofil:docker": "npx nx build benutzerprofil -c docker --output-hashing=all --base-href /benutzerprofil/ --skip-nx-cache",
    "build:benutzerprofil:qs": "npx nx build benutzerprofil -c heikeqs --output-hashing=all --base-href /benutzerprofil/ --skip-nx-cache",
    "build:benutzerprofil:prod": "npx nx build benutzerprofil:production --output-hashing=all --base-href /benutzerprofil/ --skip-nx-cache",
    "test": "npx nx test",
    "test:all": "npx nx run-many --target=test --all",
    "lint:all": "npx nx run-many --target=lint --all",
    "full-check": "npx nx run-s lint:all test:all build",
    "graph": "npx nx graph"
  },
  "private": true,
